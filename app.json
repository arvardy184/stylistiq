{
  "expo": {
    "name": "Stylistiq",
    "slug": "stylistiq",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Stylistiq needs camera access to analyze your outfits",
        "NSPhotoLibraryUsageDescription": "Stylistiq needs photo library access to select outfit photos"
      }
    },
    "android": {
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": ["expo-camera", "expo-image-picker", "expo-font"]
  }
}
